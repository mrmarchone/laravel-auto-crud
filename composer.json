{
    "name": "mrmarchone/laravel-auto-crud",
    "description": "Laravel Auto CRUD helps you streamline development and save time.",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0",
        "laravel/prompts": "*",
        "spatie/laravel-data": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mrmarchone\\LaravelAutoCrud\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Abdelrahman Muhammed",
            "email": "mrmarchone@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Mrmarchone\\LaravelAutoCrud\\LaravelAutoCrudServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^3.7",
        "orchestra/testbench": "^9.9"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
